{
  "mappings": [
    {
      "request": {
        "method": "POST",
        "urlPathPattern": "/signup",
        "headers": {
          "SCENARIO_KEY": {
            "matches": "DA-154.*"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json",
          "SCENARIO_KEY": "{{request.headers.SCENARIO_KEY}}"
        }
      }
    }
  ]
}